{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "TEMP_DIR": "/tmp/latex-temp",
    "MAX_CONCURRENT_JOBS": "5"
  }
} 